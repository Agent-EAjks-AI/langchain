{
  "name": "docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "rm -rf ./docs/api && docusaurus start",
    "build": "make vercel-build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "lint": "eslint --cache \"**/*.js\"",
    "lint:fix": "yarn lint --fix",
    "precommit": "lint-staged",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,md,mdx}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,md,mdx}\"",
    "gen": "yarn gen:supabase",
    "gen:supabase": "npx supabase gen types typescript --project-id 'xsqpnijvmbodcxyapnyq' --schema public > ./src/supabase.d.ts",
    "check-broken-links": "make vercel-build && node ./scripts/check-broken-links.js"
  },
  "dependencies": {
    "@docusaurus/core": "3.8.1",
    "@docusaurus/plugin-google-tag-manager": "^3.8.1",
    "@docusaurus/preset-classic": "3.8.1",
    "@docusaurus/remark-plugin-npm2yarn": "^3.8.1",
    "@docusaurus/theme-mermaid": "3.8.1",
    "@mdx-js/react": "^3.1.0",
    "@supabase/supabase-js": "^2.52.0",
    "clsx": "^2.1.1",
    "cookie": "^1.0.2",
    "cytoscape": "3.32.1",
    "isomorphic-dompurify": "^2.26.0",
    "json-loader": "^0.5.7",
    "prism-react-renderer": "^2.4.1",
    "process": "^0.11.10",
    "raw-loader": "^4.0.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "typescript": "^5.8.3",
    "uuid": "^11.1.0",
    "webpack": "^5.100.2"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.28.0",
    "@langchain/scripts": "^0.1.3",
    "docusaurus-plugin-typedoc": "1.4.0",
    "dotenv": "^17.2.0",
    "eslint": "^9.31.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "marked": "^16.1.1",
    "prettier": "^3.6.2",
    "supabase": "^2.31.8",
    "typedoc": "^0.28.7",
    "yaml-loader": "^0.8.0"
  },
  "resolutions": {
    "cytoscape": "3.28.1"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
